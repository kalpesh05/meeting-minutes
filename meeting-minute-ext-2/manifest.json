{
  "manifest_version": 3,
  "name": "Meet Audio Recorder2",
  "version": "1.0",
  "description": "Capture Google Meet audio",
  "permissions": ["tabCapture", "tabs", "activeTab", "scripting"],
  "host_permissions": ["https://meet.google.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
